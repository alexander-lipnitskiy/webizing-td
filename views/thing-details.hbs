<h4><a href="{{model.[@context]}}{{model.[@type]}}">{{model.name}}</a> </h4>
<h5>Description</h5>
<p>{{model.description}}</p>
<h5>Security</h5>

<table class="table table-bordered">
    <thead>
    <tr>
        <th>Type</th>
        <th scope="col">Security mechanism</th>
        <th scope="col">Description</th>
    </tr>
    </thead>

    <tbody>
    {{#each model.securityDefinitions}}
        <tr>
            <td>{{@key}}</td>
            <td>{{this.scheme}}</td>
            <td>{{this.description}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>



<h5>Properties</h5>

<table class="table table-bordered">
    <thead>
    <tr>
        <th scope="col">Property</th>
        <th scope="col">Type</th>
        <th scope="col">Description</th>
        <th scope="col">ReadOnly</th>
    </tr>
    </thead>

    <tbody>
        {{#each model.properties}}
            <tr>
                <td><a href="{{../model.[@context]}}{{this.[@type]}}">{{this.title}}</a></td>
                <td>{{this.type}}</td>
                <td>{{this.description}}</td>
                <td>{{this.readOnly}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>

<h5>Forms</h5>

<table class="table table-bordered">
    <thead>
    <tr>
        <th>Type</th>
        <th scope="col">Security mechanism</th>
        <th scope="col">Description</th>
    </tr>
    </thead>

    <tbody>
    {{#each model.securityDefinitions}}
        <tr>
            <td>{{@key}}</td>
            <td>{{this.scheme}}</td>
            <td>{{this.description}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>