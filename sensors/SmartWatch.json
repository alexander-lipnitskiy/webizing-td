{
  "id": "https://.../td/SmartWatch",
  "@context": "https://schema.iot.webizing.org/",
  "@type": ["SmartWatch"],
  "name": "SmartWatch Sensor",
  "names": "",
  "description" : "걸음 수, 심박, 운동시간, 일어서 있는 시간 로깅 ex)Fitbit, AppleWatch",
  "properties": {
    "name": {
      "@type": "name",
      "type": "string",
      "title": "Name",
      "description": "name of the sensor - format: firstName+LastName ex)jonghoLee, wanhoIm",
      "forms": [
        {
          "href": "http://localhost:3000/graphql?query={ smartWatch {name}}",
          "op": ["readproperty"], "contentType": "application/json"
        }
      ],
      "readOnly": true
    },
    "user": {
      "@type": "user",
      "type": "string",
      "title": "User",
      "description": "user of the sensor - format: firstName+LastName ex)jonghoLee, wanhoIm",
      "forms": [{
          "href": "http://localhost:3000/graphql?query={ smartWatch {user}}",
          "op": ["readproperty"], "contentType": "application/json"
        }],
      "readOnly": true
    },
    "address": {
      "@type": "address",
      "type": "string",
      "title": "Address",
      "description": "building where the sensor resides - format: organization-building ex)yongjaelee-house, kist-l1, kist-l8",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch { address }}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    },
    "room": {
      "type": "room",
      "@type": "schema:Text",
      "title": "Room",
      "description": "room number of the building where the sensor resides - ex) L8321",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch {room }}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    },
    "location": {
      "@type": "location",
      "type": "string",
      "title": "Location",
      "description": "location of the sensor - format: free",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch { location }}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    },
    "time": {
      "@type": "time",
      "type": "string",
      "title": "Time",
      "description": "time at which the data measurement was made - ex)\"2017-05-30T18:54:20+09:00\"",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch { time }}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    },
    "startDate": {
      "@type": "startDate",
      "type": "string",
      "title": "Start Date",
      "description": "start time ex)2017-09-10T00:00:00+09:00",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch {startDate }}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    },
    "endDate": {
      "@type": "endDate",
      "type": "string",
      "title": "End Date",
      "description": "sleep end time ex)2017-09-10T06:30:00+09:00",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch {endDate}}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    },
    "stepCount": {
      "@type": "stepCount",
      "type": "integer",
      "title": "Step Count",
      "description": "걸음 수",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch {stepCount}}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    },
    "heartRate": {
      "@type": "heartRate",
      "type": "integer",
      "title": "Heart Rate",
      "description": "심박수 - unit: count/min",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch { heartRate }}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    },
    "exerciseTime": {
      "@type": "exerciseTime",
      "type": "number",
      "title": "Exercise Time",
      "description": "운동시간 - unit: minute",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch { exerciseTime }}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    },
    "standHour": {
      "@type": "standHour",
      "type": "number",
      "title": "Stand Hour",
      "description": "일어선 시간 - 1: 1시간, 0: 0시간",
      "forms": [{"href": "http://localhost:3000/graphql?query={ smartWatch {standHour}}", "op": ["readproperty"], "contentType": "application/json"}],
      "readOnly": true
    }
  },
  "forms": [
    {
      "href": "http://localhost:3000/graphql?query={ smartWatch {name user   address    room   location    time    startDate   endDate stepCount heartRate exerciseTime standHour}}",
      "contentType": "application/json",
      "op": ["readallproperties"]
    }
  ],
  "securityDefinitions": {"nosec_sc": {"description" : "no security","scheme": "nosec"}},
  "security": ["nosec_sc"],
  "version": {"instance":"0.0.1" }
}
